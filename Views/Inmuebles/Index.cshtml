@model IEnumerable<Inmobiliaria.Models.Inmuebles>

@{
    ViewData["Title"] = "Inmuebles";
}

<h1>Inmuebles</h1>

<p>
    <a asp-action="Create">Agregar Inmueble</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.IdInm)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdProp)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Direccion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Tipo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Uso)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CantAmbientes)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Costo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Disponible)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IdInm)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdProp)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Direccion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Tipo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Uso)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CantAmbientes)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Costo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Disponible)
            </td>
            <td>
                @Html.ActionLink("Editar", "Edit", new { id=item.IdInm }) |
                @Html.ActionLink("Detalles", "Details", new { id=item.IdInm }) |
                @Html.ActionLink("Eliminar", "Delete", new { id=item.IdInm })
            </td>
        </tr>
}
    </tbody>
</table>
